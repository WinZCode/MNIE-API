{
  "name": "mnie-project",
  "private": true,
  "version": "1.0.0",
  "author": "winzcode",
  "description": "winzcode",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src --extensions \".ts,.tsx\" --verbose",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && cross-env BUILD_MODE=prod node ./build/src/server.js",
    "dev": "cross-env BUILD_MODE=dev nodemon --exec babel-node --extensions \".ts,.tsx\" src/server.ts"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "async-exit-hook": "^2.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "mongodb": "^6.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/node": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/async-exit-hook": "^2.0.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.4",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.47.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
